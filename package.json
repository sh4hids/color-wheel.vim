{
  "name": "color-wheel",
  "version": "1.0.0",
  "description": "A simple script to generate colorscheme for (neo)vim",
  "main": "build.js",
  "scripts": {
    "build": "node build.js",
    "lint": "eslint --ignore-path .gitignore --ext .js",
    "prettier": "prettier --ignore-path .gitignore \"**/*.+(js|json)\"",
    "format": "npm run prettier -- --write",
    "check-format": "npm run prettier -- --list-different",
    "test": "jest",
    "validate": "npm-run-all --parallel check-format lint test build"
  },
  "engines": {
    "node": "16"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sh4hids/color-wheel.git"
  },
  "keywords": [],
  "author": "Shahidul Islam Majumder <hello@shahid.pro>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sh4hids/color-wheel/issues"
  },
  "homepage": "https://github.com/sh4hids/color-wheel#readme",
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.17.10",
    "@babel/preset-env": "^7.17.10",
    "@babel/register": "^7.17.7",
    "babel-eslint": "^10.1.0",
    "eslint": "^8.15.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.1.5",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^28.1.0",
    "npm-run-all": "^4.1.5"
  }
}
